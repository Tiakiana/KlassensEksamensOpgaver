@model IEnumerable<MvcWebApplication.Models.BlogPost>

@{
    ViewBag.Title = "NewSite";
}

<h2>@Session["AuthorName"]'s Posts</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>@item.Author</td>
        <td>@item.Title</td>
        <td>@item.Content</td>
        <td>@item.CreateDate</td>

    </tr>
}

</table>
